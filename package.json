  "svelte": "src/svelthree.js",
  "module": "dist/svelthree.mjs",
  "main": "dist/svelthree.js",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {

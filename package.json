{
	"name": "svelthree",
	"version": "1.0.0-next.0.55",
	"description": "Svelte powered three.js development",
	"module": "index.js",
	"main": "index",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./src/index.d.ts",
			"import": "./index.js"
		},
		"./utils": {
			"types": "./utils/index.d.ts",
			"import": "./utils/index.js"
		},
		"./stores": {
			"types": "./stores.d.ts",
			"import": "./stores.js"
		}
	},
	"files": [
		"index.js",
		"index.d.ts",
		"stores.js",
		"stores.d.ts",
		"utils",
		"src",
		"NEED_HELP.md",
		"THREE_COMPATIBILITY.md",
		"CHANGELOG.md",
		"README.md"
	],
	"scripts": {
		"svelte-check": "svelte-check",
		"prettier-all": "prettier --config ./.prettierrc --end-of-line crlf --write src/**/*.{svelte,ts}",
		"pack-to-mynpm": "npm pack --pack-destination ../__my_npm/",
		"test": "echo 'No test specified' && exit 1"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.37",
		"fs-extra": "^10.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte-check": "^2.4.3",
		"svelte-preprocess": "^4.10.2",
		"typescript": "^4.5.5",
		"vite": "^2.7.13"
	},
	"peerDependencies": {
		"svelte": "3.44.2 - 3.46.4",
		"svelte-accmod-patch": "^1.0.0",
		"three": "0.125.x - 0.137.x"
	},
	"optionalDependencies": {
		"@types/three": "0.125.x - 0.137.x"
	},
	"author": "Vatroslav Vrbanic",
	"repository": {
		"type": "git",
		"url": "https://github.com/vatro/svelthree"
	},
	"homepage": "https://svelthree.dev",
	"bugs": {
		"url": "https://github.com/vatro/svelthree/issues"
	},
	"license": "MIT",
	"keywords": [
		"svelte",
		"svelte.js",
		"three",
		"three.js",
		"svelthree",
		"svelte-accmod"
	]
}

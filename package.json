{
  "name": "svelthree",
  "version": "0.119.1",
  "description": "Svelte powered three.js development",
  "svelte": "src/svelthree.js",
  "types": "src/svelthree.d.ts",
  "module": "dist/svelthree.mjs",
  "main": "dist/svelthree.js",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "svelte-check": "svelte-check",
    "prettier-all": "prettier --config ./.prettierrc --end-of-line crlf --write src/**/*.{svelte,ts}",
    "local-tsc": "tsc",
    "declarations": "tsc --declaration --emitDeclarationOnly --removeComments true --declarationDir ./src/types",
    "docs": "typedoc --out docs src",
    "svelthree-three-types": "node generate-svelthree-three-types.js",
    "test": "echo 'No test specified' && exit 1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "copy-dir": "^1.3.0",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.29.0",
    "svelte-check": "^1.0.52",
    "svelte-preprocess": "^4.3.2",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "three-mesh-bvh": "git+https://github.com/gkjohnson/three-mesh-bvh.git"
  },
  "author": "Vatroslav Vrbanic",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatro/svelthree"
  },
  "homepage": "https://svelthree.dev",
  "bugs": {
    "url": "https://github.com/vatro/svelthree/issues"
  },
  "license": "MIT",
  "keywords": [
    "svelte",
    "svelte.js",
    "three",
    "three.js",
    "svelthree"
  ]
}

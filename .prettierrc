{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 120,
	"semi": false,
	"endOfLine": "crlf",
	"pluginSearchDirs": ["."],
	"bracketSameLine": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": "src/lib/types/*.ts",
			"options": {
				"printWidth": 160
			}
		}
	]
}
